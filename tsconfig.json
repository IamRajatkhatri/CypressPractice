{
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig to read more about this file */
    "types": ["node", "cypress", "cypress-xpath", "cypress-visual-regression",  "@shelex/cypress-allure-plugin/reporter"],
    //"outDir": "./dist",
    "resolveJsonModule": true,
    "target": "ES5",
    //"lib": ["es6", "dom"],
    "module": "Node16",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true, 
    "moduleResolution": "Node16",
    
  },
 
  "include": ["**/*.ts", "cypress/**/*.ts", "cypress/support/commands.ts"]
}
